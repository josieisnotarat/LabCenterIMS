{
  "name": "lab-center-ims",
  "version": "1.0.0",
  "private": true,
  "description": "API server for the Lab Center IMS demo UI",
  "main": "electron/main.js",
  "scripts": {
    "start": "node server.js",
    "start:electron": "electron .",
    "dist": "electron-builder"
  },
  "dependencies": {
    "better-sqlite3": "^11.7.0",
    "electron": "^30.5.1",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "edu.cincinnatistate.labcenterims",
    "productName": "LabCenterIMS",
    "files": [
      "LabCenterIMS.html",
      "server.js",
      "db/**/*",
      "electron/**/*",
      "package.json"
    ],
    "directories": {
      "output": "dist"
    }
  }
}
